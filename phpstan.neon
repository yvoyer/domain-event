includes:
    - vendor/phpstan/phpstan-phpunit/extension.neon
    - vendor/phpstan/phpstan-phpunit/rules.neon

parameters:
    paths:
        - src
    level: max
    reportUnmatchedIgnoredErrors: false
    ignoreErrors:
        - '#Anonymous class extends unknown class Symfony\\Component\\EventDispatcher\\Event.#'
    excludes_analyse:
        - '%rootDir%/tests/'
